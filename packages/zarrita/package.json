{
  "name": "zarrita",
  "type": "module",
  "license": "MIT",
  "version": "0.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/manzt/zarrita.js.git"
  },
  "sideEffects": false,
  "scripts": {
    "test": "uvu -r tsm tests",
    "check": "tsc --noEmit",
    "prepack": "mkdist --declaration",
    "fmt": "deno fmt --options-use-tabs --options-line-width 90 src tests README.md"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./v2": {
      "types": "./dist/v2.d.ts",
      "import": "./dist/v2.mjs"
    },
    "./v3": {
      "types": "./dist/v3.d.ts",
      "import": "./dist/v3.mjs"
    },
    "./ops": {
      "types": "./dist/ops.d.ts",
      "import": "./dist/ops.mjs"
    },
    "./ndarray": {
      "types": "./dist/ndarray.d.ts",
      "import": "./dist/ndarray.mjs"
    },
    "./storage/*": {
      "types": "./dist/storage/*.d.ts",
      "import": "./dist/storage/*.mjs"
    },
    "./custom-arrays": {
      "types": "./dist/lib/custom-arrays.d.ts",
      "import": "./dist/lib/custom-arrays.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@types/ndarray": "^1.0.11",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "numcodecs": "^0.2.2",
    "reference-spec-reader": "^0.1.3",
    "unzipit": "^1.3.6"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@types/node": "^16.10.2",
    "mkdist": "^0.3.5",
    "tsm": "^2.2.1",
    "typescript": "^4.6.2",
    "uvu": "^0.5.3"
  }
}
