{
	"name": "@zarrita/storage",
	"type": "module",
	"version": "0.0.2",
	"sideEffects": false,
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist"
	},
	"main": "dist/src/index.js",
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
