	"devDependencies": {
		"@types/node": "^17.0.35",
		"@types/wicg-file-system-access": "^2020.9.5",
		"@zarrita/types": "workspace:*"
	},
	"dependencies": {
		"reference-spec-reader": "^0.1.3",
		"unzipit": "^1.4.0"
	}
}
