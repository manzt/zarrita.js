{
	"name": "@zarrita/storage",
	"type": "module",
	"version": "0.0.2",
	"sideEffects": false,
	"files": ["dist"],
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./*": {
			"types": "./src/*.ts",
			"import": "./src/*.ts"
		}
	},
	"dependencies": {
		"reference-spec-reader": "^0.2.0",
		"unzipit": "^1.3.6"
	},
	"publishConfig": {
		"main": "dist/src/index.js",
		"exports": {
			".": {
				"types": "./dist/src/index.d.ts",
				"import": "./dist/src/index.js"
			},
			"./*": {
				"types": "./dist/src/*.d.ts",
				"import": "./dist/src/*.js"
			}
		}
	}
}
