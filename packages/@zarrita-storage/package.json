			},
			"./fs": {
				"types": "./dist/src/fs.d.ts",
				"import": "./dist/src/fs.js"
			},
			"./ref": {
				"types": "./dist/src/ref.d.ts",
				"import": "./dist/src/ref.js"
			},
			"./zip": {
				"types": "./dist/src/zip.d.ts",
