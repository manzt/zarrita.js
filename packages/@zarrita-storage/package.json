			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./fetch": {
			"types": "./src/fetch.ts",
			"import": "./src/fetch.ts"
		},
		"./fs": {
			"types": "./src/fs.ts",
			"import": "./src/fs.ts"
		},
		"./ref": {
			"types": "./src/ref.ts",
