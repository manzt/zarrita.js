{
  "name": "zarrita",
  "type": "module",
  "license": "MIT",
  "version": "0.2.0-beta.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/manzt/zarrita.js.git"
  },
  "sideEffects": false,
  "scripts": {
    "test": "node ./test/index.test.js",
    "check": "tsc --noEmit",
    "prepare": "tsc --emitDeclarationOnly && cp ./src/types.d.ts ./types/types.d.ts"
  },
  "files": [
    "src/",
    "types/"
  ],
  "exports": {
    ".": "./src/index.js",
    "./v2": "./src/v2.js",
    "./v3": "./src/v3.js",
    "./ndarray": "./src/ndarray.js",
    "./storage/*": "./src/storage/*.js"
  },
  "typesVersions": {
    "*": {
      "*": ["types/*"]
    }
  },
  "dependencies": {
    "@types/ndarray": "^1.0.11",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "numcodecs": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "zora": "^5.0.0"
  }
}
