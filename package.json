{
  "name": "zarrita",
  "type": "module",
  "license": "MIT",
  "version": "0.2.0-alpha.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/manzt/zarrita.js.git"
  },
  "sideEffects": false,
  "scripts": {
    "test": "mkdist && node ./test/index.test.js",
    "check": "tsc --noEmit",
    "prepack": "mkdist --declaration",
    "fmt":  "deno fmt --options-use-tabs --options-line-width 90 src test"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./v2": "./dist/v2.mjs",
    "./v3": "./dist/v3.mjs",
    "./ndarray": "./dist/ndarray.mjs",
    "./storage/*": "./dist/storage/*.mjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@types/ndarray": "^1.0.11",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "numcodecs": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "mkdist": "^0.3.5",
    "typescript": "^4.5.2",
    "zora": "^5.0.0"
  }
}
