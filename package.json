{
  "name": "zarrita",
  "type": "module",
  "license": "MIT",
  "version": "0.2.0-beta.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/manzt/zarrita.js.git"
  },
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "node ./test/index.test.js",
    "check": "tsc --noEmit",
    "prepare": "tsc --emitDeclarationOnly && cp ./src/types.d.ts ./types/types.d.ts"
  },
  "files": [
    "src/",
    "types/"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./v2": {
      "types": "./types/v2.d.ts",
      "import": "./src/v2.js"
    },
    "./v3": {
      "types": "./types/v3.d.ts",
      "import": "./src/v3.js"
    },
    "./ndarray": {
      "types": "./types/ndarray.d.ts",
      "import": "./src/ndarray.js"
    },
    "./storage/*": {
      "types": "./types/storage/*.d.ts",
      "import": "./src/storage/*.js"
    }
  },
  "dependencies": {
    "@types/ndarray": "^1.0.11",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "numcodecs": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "zora": "^5.0.0"
  }
}
